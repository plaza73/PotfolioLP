<header class="masthead mb-auto">
  <div class="inner">
   <h3 class="masthead-brand"><p style="font-weight:900;">Lorenzo Palazzina</p></h3>
   <nav class="nav nav-masthead justify-content-center">
       <%= link_to "Home", root_path, class: 'nav-link' %>
       <%= link_to "Blog", blogs_path, class: 'nav-link' %>
       <%= link_to "About", about_path, class: 'nav-link' %>
       <%= link_to "Contact", contact_path, class: 'nav-link' %>
       
       
      <% if current_user %>
        <%= link_to "Logout", destroy_user_session_path, class: 'nav-link' %>
        <%= link_to "Edit user", edit_user_registration_path, class: 'nav-link' %>
      <% else %>
        <%= link_to "Register", new_user_registration_path, class: 'nav-link' %> 
        <%= link_to "Login", new_user_session_path, class: 'nav-link' %>
      <% end %>
   </nav>
  </div>
</header>  

